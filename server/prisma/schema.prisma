// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./qt.db"
}

model Venues {
  id Int @id @default(autoincrement())
  name String
  location String
}

model Teams {
  id Int @id @default(autoincrement())
  name String
}  