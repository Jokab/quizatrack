{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "build": "prisma generate --schema server/prisma/schema.prisma && nuxi build && recreate-db",
    "recreate-db": "mkdir tmp && cp -r server/prisma/migrations server/prisma/schema.prisma server/prisma/script.ts tmp && cd tmp && yarn prisma migrate deploy && cp qt.db ../.output/server && cd .. && rm -rf tmp",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && prisma generate --schema server/prisma/schema.prisma",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@nuxt/eslint-config": "^0.2.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "^5.10.1",
    "@types/eslint": "^8.56.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.17",
    "autoprefixer": "^10.4.17",
    "chart.js": "^4.4.1",
    "dotenv": "^16.4.2",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "lodash": "^4.17.21",
    "nuxt": "^3.10.1",
    "nuxt-module-eslint-config": "^0.1.0",
    "nuxt-primevue": "^0.3.0",
    "pinia": "^2.1.7",
    "postcss": "^8.4.35",
    "primeicons": "^6.0.1",
    "primevue": "^3.49.0",
    "prisma": "^5.10.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vue": "^3.4.18",
    "vue-router": "^4.3.0",
    "vue-tsc": "^1.8.27"
  }
}
